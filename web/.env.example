APP_NAME=ChatBotKinhTe
APP_ENV=local
APP_KEY=base64:
APP_DEBUG=true
APP_URL=http://localhost:8000

# APP_ENV=production
# VITE_DEV_SERVER=false

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=INFO

DB_CONNECTION=mysql
QUEUE_CONNECTION=sync
USE_MYSQL_VECTOR_STORE=true
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=he_thong_chatbot_kinh_te
DB_USERNAME=he_thong_chatbot_kinh_te
DB_PASSWORD=

# Cấu hình Chatbot
DB_FAISS_PATH=vector_db
MODEL_PATH=models/mistral-7b-instruct-v0.1.Q2_K.gguf
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
TEMPERATURE=0.2
MAX_TOKENS=512
N_CTX=4096
N_GPU_LAYERS=10
N_BATCH=512
F16_KV=true
USE_MMAP=false
USE_MLOCK=true

# API Configuration
USE_API=true
API_TYPE=google
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_MODEL=gemini-pro
API_TIMEOUT=30
API_HOST=0.0.0.0
API_PORT=8080
API_V1_STR=/api/v1
CHATBOT_API_URL=
# CHATBOT_API_URL=http://localhost:55050/api/v1/chat/chat-direct

# HuggingFace Model
HF_MODEL_ID=TinyLlama/TinyLlama-1.1B-Chat-v1.0

# Bảo mật
SECRET_KEY=your-super-secret-key-change-this-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS Settings
CORS_ORIGINS=["*"]
CORS_METHODS=["*"]
CORS_HEADERS=["*"]

# Session
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Hệ thống
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
CACHE_STORE=file

# GPU settings
PRIORITIZE_GPU=True
FORCE_GPU=true
SKIP_EMBEDDINGS=false

# Vite Configuration
VITE_APP_NAME="${APP_NAME}"
VITE_APP_URL="${APP_URL}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
